<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data noupdate="1">

    <record id="bring_demo_box" model="product.packaging">
        <field name="name">Bring Demo Box</field>
        <field name="height">10</field>
        <field name="width">10</field>
        <field name="max_weight">100</field>
    </record>

    <record id="product_product_delivery_bring_demo" model="product.product">
      <field name="name">Bring Delivery Demo</field>
      <field name="default_code">Delivery</field>
      <field name="type">service</field>
      <field name="categ_id" ref="product.product_category_all"/>
      <field name="sale_ok" eval="False"/>
      <field name="purchase_ok" eval="False"/>
      <field name="list_price">0.0</field>
    </record>

    <record id="delivery_carrier_bring_demo" model="delivery.carrier">
        <field name="name">Bring Test Delivery</field>
        <field name="product_id" ref="delivery_bring.product_product_delivery_bring_demo"/>
        <field name="delivery_type">bring</field>
        <field name="bring_delivery_type">BUSINESS_PARCEL</field>
    </record>

</data>
</odoo>
